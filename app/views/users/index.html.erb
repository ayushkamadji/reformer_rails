<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Password</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><%= link_to 'edit', edit_user_path(user) %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'create new', new_user_path  %>
